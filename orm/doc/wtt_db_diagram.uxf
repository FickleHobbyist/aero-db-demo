RunSchedule
--
RunNumber: INT PRIMARY KEY
CompanyTestID: INT NOT NULL REFERENCES TestEvents(CompanyID)
ConfigurationID: UUID REFERENCES Configurations(ID)
SweepScheduleSymbol: TEXT REFERENCES TestPointSchedule(Symbol)
